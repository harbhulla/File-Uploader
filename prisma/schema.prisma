generator client {
  provider               = "prisma-client-js"
  output                 = "../src/generated/prisma"
  generatedFileExtension = "js"
  moduleFormat           = "esm"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}
